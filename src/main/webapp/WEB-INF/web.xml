<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Lab 9</display-name>
    
    <context-param>
        <description>url</description>
        <param-name>URL1</param-name>
        <param-value>http://localhost:8080/lab9/task1.html</param-value>
    </context-param>
    <context-param>
        <description>url</description>
        <param-name>URL2</param-name>
        <param-value>http://localhost:8080/lab9/Task1</param-value>
    </context-param>

    <servlet>
        <servlet-name>Task2</servlet-name>
        <servlet-class>Task2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Task2</servlet-name>
        <url-pattern>/Task2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Task1</servlet-name>
        <servlet-class>Task1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Task1</servlet-name>
        <url-pattern>/Task1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ccc</servlet-name>
        <servlet-class>Ccc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ccc</servlet-name>
        <url-pattern>/Ccc</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>F1</filter-name>
        <filter-class>filters.F1</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F1</filter-name>
        <servlet-name>Ccc</servlet-name>
    </filter-mapping>

    <filter>
        <filter-name>F2</filter-name>
        <filter-class>filters.F2</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F2</filter-name>
        <url-pattern>/ccc.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>F2</filter-name>
        <servlet-name>Ccc</servlet-name>
    </filter-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
