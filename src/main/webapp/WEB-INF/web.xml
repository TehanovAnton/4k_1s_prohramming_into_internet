<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Lab6</display-name>
    
    <context-param>
        <description>url</description>
        <param-name>URL1</param-name>
        <param-value>http://localhost:8080/lab6/task1.html</param-value>
    </context-param>
    <context-param>
        <description>url</description>
        <param-name>URL2</param-name>
        <param-value>http://localhost:8080/lab6/Task1</param-value>
    </context-param>

    <servlet>
        <servlet-name>Task2</servlet-name>
        <servlet-class>Task2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Task2</servlet-name>
        <url-pattern>/Task2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Task1</servlet-name>
        <servlet-class>Task1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Task1</servlet-name>
        <url-pattern>/Task1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ccc</servlet-name>
        <servlet-class>Ccc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ccc</servlet-name>
        <url-pattern>/Ccc</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
